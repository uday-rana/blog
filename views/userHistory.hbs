<div class="row">
  <div class="col-md card">
    <div class="card-body">
      <h1 class="card-title">User History</h1>
      <div class="table-responsive">
        <table class="table">
          <thead>
            <th scope="col">Login Date/Time</th>
            <th scope="col">Client Information</th>
          </thead>
          <tbody>
            {{#each user.loginHistory}}
              <tr>
                <td>{{dateTime}}</td>
                <td>{{userAgent}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>